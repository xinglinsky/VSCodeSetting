{
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "none",
  "terminal.integrated.gpuAcceleration": "off",
  "redhat.telemetry.enabled": true,
  "files.autoSave": "onFocusChange",
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "cmake.showConfigureWithDebuggerNotification": false,
  "cmake.additionalCompilerSearchDirs": [
    "C:/msys64/mingw32/bin",
    "C:/msys64/mingw64/bin",
    "C:/msys64/clang32/bin",
    "C:/msys64/clang64/bin",
    "C:/msys64/clangarm64/bin",
    "C:/msys64/ucrt64/bin"
  ],
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "clangd.path": "c:\\Users\\yuanlong.hu\\AppData\\Roaming\\Cursor\\User\\globalStorage\\llvm-vs-code-extensions.vscode-clangd\\install\\21.1.0\\clangd_21.1.0\\bin\\clangd.exe",
  "clangd.enableCodeCompletion": false,
  "clangd.inactiveRegions.useBackgroundHighlight": true,
  "git.rebaseWhenSync": true,
  "git.terminalGitEditor": true,
  "git.autofetch": true,
  "git.fetchOnPull": true,
  "git.pruneOnFetch": true,
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.targets.enable": true,
  "vim.useCtrlKeys": false,
  "vim.leader": "\u003cspace\u003e",
  "vim.sneakReplacesF": true,
  "vim.smartRelativeLine": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.highlightedyank.enable": true,
  "vim.useSystemClipboard": true,
  "vim.overrideCopy": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "\u003cEsc\u003e"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "g",
        "m"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "g",
        "o"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "g",
        "h"
      ],
      "commands": [
        "clangd.switchheadersource"
      ]
    },
    {
      "before": [
        "\u003cleader\u003e",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "\u003cleader\u003e",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "\u003cleader\u003e",
        "m"
      ],
      "commands": [
        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "\u003cleader\u003e",
        "b"
      ],
      "commands": [
        "bookmarks.list"
      ]
    },
    {
      "before": [
        "\u003cleader\u003e",
        "w",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "\u003cleader\u003e",
        "w",
        "o"
      ],
      "commands": [
        "workbench.action.output.toggleOutput"
      ]
    },
    {
      "before": [
        "\u003cleader\u003e",
        "w",
        "p"
      ],
      "commands": [
        "workbench.actions.view.problems"
      ]
    }
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "syncSettings.resources": [
    "extensions",
    "keybindings",
    "settings",
    "snippets",
    "uiState"
  ],
  "syncSettings.keybindingsPerPlatform": true,
  "go.toolsManagement.autoUpdate": true,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "terminal.integrated.profiles.windows": {
    "MSYS2 MinGW64": {
      "path": "C:\\msys64\\usr\\bin\\bash.exe",
      "args": [
        "--login",
        "-i"
      ],
      "env": {
        "MSYSTEM": "MINGW64",
        "CHERE_INVOKING": "1",
        "MSYS2_PATH_TYPE": "inherit"
      },
      "icon": "terminal-bash"
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.activityBar.location": "top",
  "editor.fontFamily": "Cascadia Code, Consolas, 'Courier New', monospace"
}